{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from 'axios';\nexport default {\n  name: 'DiseasePredictorApp',\n  data() {\n    return {\n      symptoms: [{\n        id: 'demam',\n        label: 'Demam',\n        sublabel: '> 37.5°C',\n        color: 'yellow',\n        iconColor: 'orange',\n        icon: 'ThermometerIcon'\n      }, {\n        id: 'batuk',\n        label: 'Batuk',\n        sublabel: 'Kering / Berdahak',\n        color: 'gray',\n        iconColor: 'blue',\n        icon: 'LungsIcon'\n      }, {\n        id: 'sakit_kepala',\n        label: 'Sakit Kepala',\n        sublabel: 'Pusing',\n        color: 'gray',\n        iconColor: 'purple',\n        icon: 'BrainIcon'\n      }, {\n        id: 'pilek',\n        label: 'Pilek',\n        sublabel: 'Hidung Tersumbat',\n        color: 'gray',\n        iconColor: 'teal',\n        icon: 'NoseIcon'\n      }, {\n        id: 'sakit_tenggorokan',\n        label: 'Sakit Tenggorokan',\n        sublabel: 'Nyeri',\n        color: 'gray',\n        iconColor: 'red',\n        icon: 'ThroatIcon'\n      }, {\n        id: 'mual',\n        label: 'Mual/Muntah',\n        sublabel: 'Perut',\n        color: 'gray',\n        iconColor: 'green',\n        icon: 'StomachIcon'\n      }, {\n        id: 'nyeri_otot',\n        label: 'Nyeri Otot',\n        sublabel: 'Pegal',\n        color: 'pink',\n        iconColor: 'pink',\n        icon: 'MuscleIcon'\n      }, {\n        id: 'sesak_napas',\n        label: 'Sesak Napas',\n        sublabel: 'Sulit Bernapas',\n        color: 'gray',\n        iconColor: 'cyan',\n        icon: 'BreathIcon'\n      }, {\n        id: 'diare',\n        label: 'Diare',\n        sublabel: 'BAB Cair',\n        color: 'gray',\n        iconColor: 'brown',\n        icon: 'DigestIcon'\n      }, {\n        id: 'ruam_kulit',\n        label: 'Ruam Kulit',\n        sublabel: 'Bintik Merah',\n        color: 'gray',\n        iconColor: 'red',\n        icon: 'SkinIcon'\n      }],\n      symptomData: {\n        demam: false,\n        batuk: false,\n        sakit_kepala: false,\n        pilek: false,\n        sakit_tenggorokan: false,\n        mual: false,\n        nyeri_otot: false,\n        sesak_napas: false,\n        diare: false,\n        ruam_kulit: false\n      },\n      prediction: null,\n      loading: false,\n      apiUrl: 'http://localhost:5000/api'\n    };\n  },\n  computed: {\n    selectedCount() {\n      return Object.values(this.symptomData).filter(Boolean).length;\n    },\n    confidenceClass() {\n      if (!this.prediction) return '';\n      const confidence = this.prediction.confidence;\n      if (confidence === 'Tinggi') return 'high';\n      if (confidence === 'Sedang') return 'medium';\n      return 'low';\n    }\n  },\n  methods: {\n    async predictDisease() {\n      this.loading = true;\n      try {\n        const response = await axios.post(`${this.apiUrl}/predict`, {\n          symptoms: this.symptomData\n        });\n        if (response.data.success) {\n          this.prediction = response.data.prediction;\n        }\n      } catch (error) {\n        console.error('Error predicting disease:', error);\n        alert('Terjadi kesalahan saat memprediksi penyakit. Pastikan backend server berjalan.');\n      } finally {\n        this.loading = false;\n      }\n    },\n    onSymptomChange() {\n      this.prediction = null;\n    },\n    resetForm() {\n      Object.keys(this.symptomData).forEach(key => {\n        this.symptomData[key] = false;\n      });\n      this.prediction = null;\n    }\n  },\n  components: {\n    ThermometerIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M14 4v10.54a4 4 0 1 1-4 0V4a2 2 0 0 1 4 0Z\"/></svg>`\n    },\n    LungsIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 2v20M8 6H6a4 4 0 0 0-4 4v4a4 4 0 0 0 4 4h2M16 6h2a4 4 0 0 1 4 4v4a4 4 0 0 1-4 4h-2\"/></svg>`\n    },\n    BrainIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><circle cx=\"12\" cy=\"12\" r=\"3\"/><path d=\"M12 2v4M12 18v4M4.93 4.93l2.83 2.83M16.24 16.24l2.83 2.83M2 12h4M18 12h4M4.93 19.07l2.83-2.83M16.24 7.76l2.83-2.83\"/></svg>`\n    },\n    NoseIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 2v8M8 6l4 4 4-4M12 10v12\"/></svg>`\n    },\n    ThroatIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><circle cx=\"12\" cy=\"8\" r=\"6\"/><path d=\"M8 14v8M16 14v8\"/></svg>`\n    },\n    StomachIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"M8 12h8M12 8v8\"/></svg>`\n    },\n    MuscleIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M6 8h12M6 12h12M6 16h12\"/></svg>`\n    },\n    BreathIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M12 2L2 7v10l10 5 10-5V7L12 2z\"/></svg>`\n    },\n    DigestIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M8 2v4M16 2v4M4 8h16M4 12h16M4 16h16M4 20h16\"/></svg>`\n    },\n    SkinIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><circle cx=\"7\" cy=\"7\" r=\"2\"/><circle cx=\"17\" cy=\"7\" r=\"2\"/><circle cx=\"7\" cy=\"17\" r=\"2\"/><circle cx=\"17\" cy=\"17\" r=\"2\"/></svg>`\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","symptoms","id","label","sublabel","color","iconColor","icon","symptomData","demam","batuk","sakit_kepala","pilek","sakit_tenggorokan","mual","nyeri_otot","sesak_napas","diare","ruam_kulit","prediction","loading","apiUrl","computed","selectedCount","Object","values","filter","Boolean","length","confidenceClass","confidence","methods","predictDisease","response","post","success","error","console","alert","onSymptomChange","resetForm","keys","forEach","key","components","ThermometerIcon","template","LungsIcon","BrainIcon","NoseIcon","ThroatIcon","StomachIcon","MuscleIcon","BreathIcon","DigestIcon","SkinIcon"],"sources":["D:\\program python\\ML\\Linear Regression\\fe\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"main-wrapper\">\n      <!-- Header -->\n      <header class=\"app-header\">\n        <div class=\"header-icon\">\n          <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"28\" height=\"28\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2.5\">\n            <path d=\"M22 12h-4l-3 9L9 3l-3 9H2\"/>\n          </svg>\n        </div>\n        <div>\n          <h1>Prediksi Penyakit</h1>\n          <p>Sistem Decision Tree menggunakan Python Backend</p>\n        </div>\n      </header>\n\n      <!-- Symptoms Selection Card -->\n      <div class=\"card symptoms-card\">\n        <div class=\"card-header\">\n          <div class=\"header-left\">\n            <div class=\"icon-circle pink\">\n              <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                <path d=\"M9 11l3 3L22 4\"/>\n                <path d=\"M21 12v7a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11\"/>\n              </svg>\n            </div>\n            <h2>Pilih Gejala yang Dialami</h2>\n          </div>\n          <div class=\"counter-badge\">{{ selectedCount }}/10</div>\n        </div>\n\n        <div class=\"symptoms-grid\">\n          <label \n            v-for=\"symptom in symptoms\" \n            :key=\"symptom.id\"\n            class=\"symptom-chip\"\n            :class=\"[symptom.color, { 'active': symptomData[symptom.id] }]\"\n          >\n            <input \n              type=\"checkbox\"\n              v-model=\"symptomData[symptom.id]\"\n              @change=\"onSymptomChange\"\n            />\n            <div class=\"chip-icon\" :class=\"symptom.iconColor\">\n              <component :is=\"symptom.icon\"></component>\n            </div>\n            <div class=\"chip-content\">\n              <span class=\"chip-label\">{{ symptom.label }}</span>\n              <span class=\"chip-sublabel\">{{ symptom.sublabel }}</span>\n            </div>\n          </label>\n        </div>\n\n        <button \n          @click=\"predictDisease\"\n          :disabled=\"selectedCount === 0 || loading\"\n          class=\"btn-predict\"\n        >\n          <svg v-if=\"!loading\" xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n            <rect x=\"3\" y=\"3\" width=\"18\" height=\"18\" rx=\"2\"/>\n            <path d=\"M9 3v18\"/>\n          </svg>\n          <span v-if=\"loading\" class=\"spinner\"></span>\n          {{ loading ? `Prediksi Penyakit (${selectedCount} gejala)` : `Prediksi Penyakit (${selectedCount} gejala)` }}\n\n        </button>\n      </div>\n\n      <!-- Result Card -->\n      <div class=\"card result-card\">\n        <div class=\"card-header\">\n          <div class=\"header-left\">\n            <div class=\"icon-circle purple\">\n              <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                <path d=\"M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z\"/>\n                <polyline points=\"14 2 14 8 20 8\"/>\n                <line x1=\"12\" y1=\"18\" x2=\"12\" y2=\"12\"/>\n                <line x1=\"9\" y1=\"15\" x2=\"15\" y2=\"15\"/>\n              </svg>\n            </div>\n            <h2>Hasil Prediksi</h2>\n          </div>\n        </div>\n\n        <div v-if=\"!prediction\" class=\"empty-state\">\n          <div class=\"empty-icon\">\n            <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"48\" height=\"48\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n              <path d=\"M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3\"/>\n              <circle cx=\"12\" cy=\"12\" r=\"10\"/>\n              <line x1=\"12\" y1=\"17\" x2=\"12.01\" y2=\"17\"/>\n            </svg>\n          </div>\n          <p>Pilih gejala yang kamu alami, kemudian klik tombol <strong>\"Prediksi Penyakit\"</strong></p>\n        </div>\n\n        <div v-else class=\"result-content\">\n          <div class=\"result-disease\">\n            <div class=\"disease-icon\">\n              <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"32\" height=\"32\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                <path d=\"M22 11.08V12a10 10 0 1 1-5.93-9.14\"/>\n                <polyline points=\"22 4 12 14.01 9 11.01\"/>\n              </svg>\n            </div>\n            <div>\n              <h3>{{ prediction.disease }}</h3>\n              <p>{{ prediction.description }}</p>\n            </div>\n          </div>\n\n          <div class=\"result-stats\">\n            <div class=\"stat-box probability\">\n              <div class=\"stat-header\">\n                <span>Probabilitas</span>\n                <span class=\"stat-value\">{{ (prediction.probability * 100).toFixed(0) }}%</span>\n              </div>\n              <div class=\"progress-bar\">\n                <div \n                  class=\"progress-fill\"\n                  :style=\"{ width: (prediction.probability * 100) + '%' }\"\n                ></div>\n              </div>\n            </div>\n\n            <div class=\"stat-box confidence\">\n              <div class=\"stat-header\">\n                <span>Tingkat Keyakinan</span>\n                <span class=\"confidence-badge\" :class=\"confidenceClass\">\n                  {{ prediction.confidence }}\n                </span>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"recommendation-box\">\n            <div class=\"rec-header\">\n              <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"18\" height=\"18\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                <circle cx=\"12\" cy=\"12\" r=\"10\"/>\n                <line x1=\"12\" y1=\"16\" x2=\"12\" y2=\"12\"/>\n                <line x1=\"12\" y1=\"8\" x2=\"12.01\" y2=\"8\"/>\n              </svg>\n              <strong>Rekomendasi Tindakan</strong>\n            </div>\n            <p>{{ prediction.recommendation }}</p>\n          </div>\n\n          <button @click=\"resetForm\" class=\"btn-reset\">\n            <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"18\" height=\"18\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n              <path d=\"M3 12a9 9 0 0 1 9-9 9.75 9.75 0 0 1 6.74 2.74L21 8\"/>\n              <path d=\"M21 3v5h-5\"/>\n              <path d=\"M21 12a9 9 0 0 1-9 9 9.75 9.75 0 0 1-6.74-2.74L3 16\"/>\n              <path d=\"M3 21v-5h5\"/>\n            </svg>\n            Reset\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'DiseasePredictorApp',\n  data() {\n    return {\n      symptoms: [\n        { \n          id: 'demam', \n          label: 'Demam', \n          sublabel: '> 37.5°C',\n          color: 'yellow',\n          iconColor: 'orange',\n          icon: 'ThermometerIcon'\n        },\n        { \n          id: 'batuk', \n          label: 'Batuk', \n          sublabel: 'Kering / Berdahak',\n          color: 'gray',\n          iconColor: 'blue',\n          icon: 'LungsIcon'\n        },\n        { \n          id: 'sakit_kepala', \n          label: 'Sakit Kepala', \n          sublabel: 'Pusing',\n          color: 'gray',\n          iconColor: 'purple',\n          icon: 'BrainIcon'\n        },\n        { \n          id: 'pilek', \n          label: 'Pilek', \n          sublabel: 'Hidung Tersumbat',\n          color: 'gray',\n          iconColor: 'teal',\n          icon: 'NoseIcon'\n        },\n        { \n          id: 'sakit_tenggorokan', \n          label: 'Sakit Tenggorokan', \n          sublabel: 'Nyeri',\n          color: 'gray',\n          iconColor: 'red',\n          icon: 'ThroatIcon'\n        },\n        { \n          id: 'mual', \n          label: 'Mual/Muntah', \n          sublabel: 'Perut',\n          color: 'gray',\n          iconColor: 'green',\n          icon: 'StomachIcon'\n        },\n        { \n          id: 'nyeri_otot', \n          label: 'Nyeri Otot', \n          sublabel: 'Pegal',\n          color: 'pink',\n          iconColor: 'pink',\n          icon: 'MuscleIcon'\n        },\n        { \n          id: 'sesak_napas', \n          label: 'Sesak Napas', \n          sublabel: 'Sulit Bernapas',\n          color: 'gray',\n          iconColor: 'cyan',\n          icon: 'BreathIcon'\n        },\n        { \n          id: 'diare', \n          label: 'Diare', \n          sublabel: 'BAB Cair',\n          color: 'gray',\n          iconColor: 'brown',\n          icon: 'DigestIcon'\n        },\n        { \n          id: 'ruam_kulit', \n          label: 'Ruam Kulit', \n          sublabel: 'Bintik Merah',\n          color: 'gray',\n          iconColor: 'red',\n          icon: 'SkinIcon'\n        }\n      ],\n      symptomData: {\n        demam: false,\n        batuk: false,\n        sakit_kepala: false,\n        pilek: false,\n        sakit_tenggorokan: false,\n        mual: false,\n        nyeri_otot: false,\n        sesak_napas: false,\n        diare: false,\n        ruam_kulit: false\n      },\n      prediction: null,\n      loading: false,\n      apiUrl: 'http://localhost:5000/api'\n    };\n  },\n  computed: {\n    selectedCount() {\n      return Object.values(this.symptomData).filter(Boolean).length;\n    },\n    confidenceClass() {\n      if (!this.prediction) return '';\n      const confidence = this.prediction.confidence;\n      if (confidence === 'Tinggi') return 'high';\n      if (confidence === 'Sedang') return 'medium';\n      return 'low';\n    }\n  },\n  methods: {\n    async predictDisease() {\n      this.loading = true;\n      \n      try {\n        const response = await axios.post(`${this.apiUrl}/predict`, {\n          symptoms: this.symptomData\n        });\n        \n        if (response.data.success) {\n          this.prediction = response.data.prediction;\n        }\n      } catch (error) {\n        console.error('Error predicting disease:', error);\n        alert('Terjadi kesalahan saat memprediksi penyakit. Pastikan backend server berjalan.');\n      } finally {\n        this.loading = false;\n      }\n    },\n    onSymptomChange() {\n      this.prediction = null;\n    },\n    resetForm() {\n      Object.keys(this.symptomData).forEach(key => {\n        this.symptomData[key] = false;\n      });\n      this.prediction = null;\n    }\n  },\n  components: {\n    ThermometerIcon: {\n      template: `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\