{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nimport { ref, computed, onMounted, watch } from 'vue';\n\n// --- Ikon SVG (disalin dari Lucide) ---\n// Ikon dari library lucide-react telah diubah menjadi komponen Vue atau fungsi yang mengembalikan SVG string\nconst apiUrl = 'http://localhost:5000/api';\n// Use the mock alert function to replace window.alert\n\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const IconActivity = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><polyline points=\"22 12 18 12 15 21 9 3 6 12 2 12\"/></svg>`;\n    const IconAlertCircle = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><line x1=\"12\" y1=\"8\" x2=\"12\" y2=\"12\"/><line x1=\"12\" y1=\"16\" x2=\"12.01\" y2=\"16\"/></svg>`;\n    const IconCheckCircle2 = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"m9 12 2 2 4-4\"/></svg>`;\n    const IconThermometer = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M14 4v10.54a4 4 0 1 1-4 0V4a2 2 0 0 1 4 0Z\"/></svg>`;\n    const IconWind = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M17.7 7.7a2.5 2.5 0 1 1 1.8 4.3H2\"/><path d=\"M21 15.5H2\"/><path d=\"M16.5 19.3h-1.8a2.5 2.5 0 0 1 0-4.6h1.8\"/></svg>`;\n    const IconBrain = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M12 5a3 3 0 1 0-7.75 2h15.5a3 3 0 1 0-7.75-2\"/><path d=\"M12 5v14\"/><path d=\"M12 19a3 3 0 1 0 7.75-2H4.25a3 3 0 1 0 7.75 2\"/></svg>`;\n    const IconDroplets = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M7 16l3-6l3 6M10 10l-1 4M15 16l3-6l3 6M18 10l-1 4M5 22h14M12 4v4\"/></svg>`;\n    const IconPill = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"m10 20-6-6v-6c0-1.1.9-2 2-2h6l6 6v6c0 1.1-.9 2-2 2h-6z\"/><path d=\"m11 11-2-2\"/></svg>`;\n    const IconLoader2 = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M21 12a9 9 0 1 1-6.219-8.56\"/></svg>`;\n    const IconXCircle = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"m15 9-6 6\"/><path d=\"m9 9 6 6\"/></svg>`;\n    const IconHeart = props => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M19 14c1.49-1.46 3-3.21 3-5.5A5.5 5.5 0 0 0 16.5 3c-1.76 0-3 .5-4.5 2-1.5-1.5-2.74-2-4.5-2A5.5 5.5 0 0 0 2 8.5c0 2.3 1.5 4.05 3 5.5l7 7Z\"/></svg>`;\n\n    // --- State Management ---\n\n    const initialSymptoms = {\n      demam: false,\n      demam_tinggi: false,\n      batuk: false,\n      sakit_kepala: false,\n      nyeri_otot: false,\n      mual: false,\n      ruam_kulit: false,\n      hidung_tersumbat: false,\n      sakit_tenggorokan: false,\n      sesak: false,\n      lemah: false,\n      berdarah: false\n    };\n    const symptoms = ref({\n      ...initialSymptoms\n    });\n    const prediction = ref(null);\n    const confidence = ref(0);\n    const loading = ref(false);\n    const backendStatus = ref('checking');\n    const customAlert = message => {\n      console.warn(\"Custom Alert:\", message);\n      // In a real Vue app, you would use a custom modal/toast library here.\n      // For this environment, we log to console.\n    };\n\n    // --- Symptom Data Structure ---\n    const symptomsList = [{\n      key: 'demam',\n      label: 'Demam',\n      icon: IconThermometer,\n      color: 'orange'\n    }, {\n      key: 'demam_tinggi',\n      label: 'Demam Tinggi (>38.5¬∞C)',\n      icon: IconThermometer,\n      color: 'red'\n    }, {\n      key: 'batuk',\n      label: 'Batuk',\n      icon: IconWind,\n      color: 'blue'\n    }, {\n      key: 'sakit_kepala',\n      label: 'Sakit Kepala',\n      icon: IconBrain,\n      color: 'purple'\n    }, {\n      key: 'nyeri_otot',\n      label: 'Nyeri Otot',\n      icon: IconActivity,\n      color: 'yellow'\n    }, {\n      key: 'mual',\n      label: 'Mual/Muntah',\n      icon: IconAlertCircle,\n      color: 'green'\n    }, {\n      key: 'ruam_kulit',\n      label: 'Ruam Kulit',\n      icon: IconDroplets,\n      color: 'pink'\n    }, {\n      key: 'hidung_tersumbat',\n      label: 'Hidung Tersumbat',\n      icon: IconWind,\n      color: 'cyan'\n    }, {\n      key: 'sakit_tenggorokan',\n      label: 'Sakit Tenggorokan',\n      icon: IconAlertCircle,\n      color: 'red'\n    }, {\n      key: 'sesak',\n      label: 'Sesak Napas',\n      icon: IconWind,\n      color: 'indigo'\n    }, {\n      key: 'lemah',\n      label: 'Lemah/Lesu',\n      icon: IconActivity,\n      color: 'gray'\n    }, {\n      key: 'berdarah',\n      label: 'Mimisan/Gusi Berdarah',\n      icon: IconDroplets,\n      color: 'red'\n    }];\n\n    // --- Computed Properties ---\n\n    const selectedCount = computed(() => {\n      return Object.values(symptoms.value).filter(v => v).length;\n    });\n\n    // --- Methods / Functions ---\n\n    // Function to check backend health\n    const checkBackendHealth = async () => {\n      backendStatus.value = 'checking';\n      try {\n        const response = await fetch(`${apiUrl}/health`);\n        if (response.ok) {\n          backendStatus.value = 'connected';\n        } else {\n          backendStatus.value = 'disconnected';\n        }\n      } catch (error) {\n        backendStatus.value = 'disconnected';\n      }\n    };\n\n    // Handle symptom toggle\n    const handleSymptomChange = key => {\n      symptoms.value[key] = !symptoms.value[key];\n      prediction.value = null;\n      confidence.value = 0;\n    };\n\n    // Handle prediction\n    const handlePredict = async () => {\n      if (selectedCount.value === 0) {\n        customAlert('‚ö†Ô∏è Pilih minimal 1 gejala untuk melakukan prediksi!');\n        return;\n      }\n      loading.value = true;\n      try {\n        const response = await fetch(`${apiUrl}/predict`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            symptoms: symptoms.value\n          })\n        });\n        const result = await response.json();\n        if (result.success) {\n          prediction.value = result.prediction;\n          confidence.value = result.confidence;\n        } else {\n          customAlert('‚ùå Error: ' + result.error);\n        }\n      } catch (error) {\n        customAlert('‚ùå Tidak dapat terhubung ke backend!\\nPastikan backend running di http://localhost:5000');\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Handle reset\n    const handleReset = () => {\n      // Reset state to initial values\n      symptoms.value = {\n        ...initialSymptoms\n      };\n      prediction.value = null;\n      confidence.value = 0;\n    };\n\n    // Utility function to get dynamic Tailwind classes for color/selection\n    const getColorClasses = (color, isSelected) => {\n      const colors = {\n        orange: isSelected ? 'bg-orange-500 border-orange-600 text-white' : 'border-orange-300 hover:border-orange-400 hover:bg-orange-50',\n        red: isSelected ? 'bg-red-500 border-red-600 text-white' : 'border-red-300 hover:border-red-400 hover:bg-red-50',\n        blue: isSelected ? 'bg-blue-500 border-blue-600 text-white' : 'border-blue-300 hover:border-blue-400 hover:bg-blue-50',\n        purple: isSelected ? 'bg-purple-500 border-purple-600 text-white' : 'border-purple-300 hover:border-purple-400 hover:bg-purple-50',\n        yellow: isSelected ? 'bg-yellow-500 border-yellow-600 text-white' : 'border-yellow-300 hover:border-yellow-400 hover:bg-yellow-50',\n        green: isSelected ? 'bg-green-500 border-green-600 text-white' : 'border-green-300 hover:border-green-400 hover:bg-green-50',\n        pink: isSelected ? 'bg-pink-500 border-pink-600 text-white' : 'border-pink-300 hover:border-pink-400 hover:bg-pink-50',\n        cyan: isSelected ? 'bg-cyan-500 border-cyan-600 text-white' : 'border-cyan-300 hover:border-cyan-400 hover:bg-cyan-50',\n        indigo: isSelected ? 'bg-indigo-500 border-indigo-600 text-white' : 'border-indigo-300 hover:border-indigo-400 hover:bg-indigo-50',\n        gray: isSelected ? 'bg-gray-500 border-gray-600 text-white' : 'border-gray-300 hover:border-gray-400 hover:bg-gray-50'\n      };\n      return colors[color] || colors.blue;\n    };\n    const getConfidenceColor = conf => {\n      if (conf >= 80) return 'from-green-400 to-green-600';\n      if (conf >= 60) return 'from-yellow-400 to-yellow-600';\n      return 'from-orange-400 to-orange-600';\n    };\n    const getConfidenceBg = conf => {\n      if (conf >= 80) return 'bg-green-50 border-green-200';\n      if (conf >= 60) return 'bg-yellow-50 border-yellow-200';\n      return 'bg-orange-50 border-orange-200';\n    };\n    const getConfidenceTextClass = conf => {\n      if (conf >= 80) return 'text-green-600';\n      if (conf >= 60) return 'text-yellow-600';\n      return 'text-orange-600';\n    };\n    const getConfidenceBadgeClass = conf => {\n      if (conf >= 80) return 'bg-green-200 text-green-800';\n      if (conf >= 60) return 'bg-yellow-200 text-yellow-800';\n      return 'bg-orange-200 text-orange-800';\n    };\n\n    // --- Lifecycle Hook ---\n    onMounted(() => {\n      checkBackendHealth();\n    });\n\n    // Watcher untuk mereset prediksi saat gejala berubah (optional, sudah ada di handleSymptomChange)\n    watch(symptoms, () => {\n      // prediction.value = null; // Ini sudah dilakukan di handleSymptomChange\n    }, {\n      deep: true\n    });\n    const __returned__ = {\n      IconActivity,\n      IconAlertCircle,\n      IconCheckCircle2,\n      IconThermometer,\n      IconWind,\n      IconBrain,\n      IconDroplets,\n      IconPill,\n      IconLoader2,\n      IconXCircle,\n      IconHeart,\n      initialSymptoms,\n      symptoms,\n      prediction,\n      confidence,\n      loading,\n      backendStatus,\n      apiUrl,\n      customAlert,\n      symptomsList,\n      selectedCount,\n      checkBackendHealth,\n      handleSymptomChange,\n      handlePredict,\n      handleReset,\n      getColorClasses,\n      getConfidenceColor,\n      getConfidenceBg,\n      getConfidenceTextClass,\n      getConfidenceBadgeClass,\n      ref,\n      computed,\n      onMounted,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","apiUrl","IconActivity","props","size","class","IconAlertCircle","IconCheckCircle2","IconThermometer","IconWind","IconBrain","IconDroplets","IconPill","IconLoader2","IconXCircle","IconHeart","initialSymptoms","demam","demam_tinggi","batuk","sakit_kepala","nyeri_otot","mual","ruam_kulit","hidung_tersumbat","sakit_tenggorokan","sesak","lemah","berdarah","symptoms","prediction","confidence","loading","backendStatus","customAlert","message","console","warn","symptomsList","key","label","icon","color","selectedCount","Object","values","value","filter","v","length","checkBackendHealth","response","fetch","ok","error","handleSymptomChange","handlePredict","method","headers","body","JSON","stringify","result","json","success","handleReset","getColorClasses","isSelected","colors","orange","red","blue","purple","yellow","green","pink","cyan","indigo","gray","getConfidenceColor","conf","getConfidenceBg","getConfidenceTextClass","getConfidenceBadgeClass","deep"],"sources":["D:/program python/ML/Linear Regression/fe/src/App.vue"],"sourcesContent":["<script setup>\nimport { ref, computed, onMounted, watch } from 'vue';\n\n// --- Ikon SVG (disalin dari Lucide) ---\n// Ikon dari library lucide-react telah diubah menjadi komponen Vue atau fungsi yang mengembalikan SVG string\nconst IconActivity = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><polyline points=\"22 12 18 12 15 21 9 3 6 12 2 12\"/></svg>`;\nconst IconAlertCircle = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><line x1=\"12\" y1=\"8\" x2=\"12\" y2=\"12\"/><line x1=\"12\" y1=\"16\" x2=\"12.01\" y2=\"16\"/></svg>`;\nconst IconCheckCircle2 = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"m9 12 2 2 4-4\"/></svg>`;\nconst IconThermometer = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M14 4v10.54a4 4 0 1 1-4 0V4a2 2 0 0 1 4 0Z\"/></svg>`;\nconst IconWind = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M17.7 7.7a2.5 2.5 0 1 1 1.8 4.3H2\"/><path d=\"M21 15.5H2\"/><path d=\"M16.5 19.3h-1.8a2.5 2.5 0 0 1 0-4.6h1.8\"/></svg>`;\nconst IconBrain = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M12 5a3 3 0 1 0-7.75 2h15.5a3 3 0 1 0-7.75-2\"/><path d=\"M12 5v14\"/><path d=\"M12 19a3 3 0 1 0 7.75-2H4.25a3 3 0 1 0 7.75 2\"/></svg>`;\nconst IconDroplets = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M7 16l3-6l3 6M10 10l-1 4M15 16l3-6l3 6M18 10l-1 4M5 22h14M12 4v4\"/></svg>`;\nconst IconPill = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"m10 20-6-6v-6c0-1.1.9-2 2-2h6l6 6v6c0 1.1-.9 2-2 2h-6z\"/><path d=\"m11 11-2-2\"/></svg>`;\nconst IconLoader2 = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M21 12a9 9 0 1 1-6.219-8.56\"/></svg>`;\nconst IconXCircle = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><circle cx=\"12\" cy=\"12\" r=\"10\"/><path d=\"m15 9-6 6\"/><path d=\"m9 9 6 6\"/></svg>`;\nconst IconHeart = (props) => `<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${props.size || 24}\" height=\"${props.size || 24}\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\" class=\"${props.class}\"><path d=\"M19 14c1.49-1.46 3-3.21 3-5.5A5.5 5.5 0 0 0 16.5 3c-1.76 0-3 .5-4.5 2-1.5-1.5-2.74-2-4.5-2A5.5 5.5 0 0 0 2 8.5c0 2.3 1.5 4.05 3 5.5l7 7Z\"/></svg>`;\n\n\n// --- State Management ---\n\nconst initialSymptoms = {\n    demam: false,\n    demam_tinggi: false,\n    batuk: false,\n    sakit_kepala: false,\n    nyeri_otot: false,\n    mual: false,\n    ruam_kulit: false,\n    hidung_tersumbat: false,\n    sakit_tenggorokan: false,\n    sesak: false,\n    lemah: false,\n    berdarah: false\n};\n\nconst symptoms = ref({ ...initialSymptoms });\nconst prediction = ref(null);\nconst confidence = ref(0);\nconst loading = ref(false);\nconst backendStatus = ref('checking');\n\nconst apiUrl = 'http://localhost:5000/api';\n// Use the mock alert function to replace window.alert\nconst customAlert = (message) => {\n    console.warn(\"Custom Alert:\", message);\n    // In a real Vue app, you would use a custom modal/toast library here.\n    // For this environment, we log to console.\n};\n\n// --- Symptom Data Structure ---\nconst symptomsList = [\n    { key: 'demam', label: 'Demam', icon: IconThermometer, color: 'orange' },\n    { key: 'demam_tinggi', label: 'Demam Tinggi (>38.5¬∞C)', icon: IconThermometer, color: 'red' },\n    { key: 'batuk', label: 'Batuk', icon: IconWind, color: 'blue' },\n    { key: 'sakit_kepala', label: 'Sakit Kepala', icon: IconBrain, color: 'purple' },\n    { key: 'nyeri_otot', label: 'Nyeri Otot', icon: IconActivity, color: 'yellow' },\n    { key: 'mual', label: 'Mual/Muntah', icon: IconAlertCircle, color: 'green' },\n    { key: 'ruam_kulit', label: 'Ruam Kulit', icon: IconDroplets, color: 'pink' },\n    { key: 'hidung_tersumbat', label: 'Hidung Tersumbat', icon: IconWind, color: 'cyan' },\n    { key: 'sakit_tenggorokan', label: 'Sakit Tenggorokan', icon: IconAlertCircle, color: 'red' },\n    { key: 'sesak', label: 'Sesak Napas', icon: IconWind, color: 'indigo' },\n    { key: 'lemah', label: 'Lemah/Lesu', icon: IconActivity, color: 'gray' },\n    { key: 'berdarah', label: 'Mimisan/Gusi Berdarah', icon: IconDroplets, color: 'red' }\n];\n\n// --- Computed Properties ---\n\nconst selectedCount = computed(() => {\n    return Object.values(symptoms.value).filter(v => v).length;\n});\n\n// --- Methods / Functions ---\n\n// Function to check backend health\nconst checkBackendHealth = async () => {\n    backendStatus.value = 'checking';\n    try {\n        const response = await fetch(`${apiUrl}/health`);\n        if (response.ok) {\n            backendStatus.value = 'connected';\n        } else {\n            backendStatus.value = 'disconnected';\n        }\n    } catch (error) {\n        backendStatus.value = 'disconnected';\n    }\n};\n\n// Handle symptom toggle\nconst handleSymptomChange = (key) => {\n    symptoms.value[key] = !symptoms.value[key];\n    prediction.value = null;\n    confidence.value = 0;\n};\n\n// Handle prediction\nconst handlePredict = async () => {\n    if (selectedCount.value === 0) {\n        customAlert('‚ö†Ô∏è Pilih minimal 1 gejala untuk melakukan prediksi!');\n        return;\n    }\n\n    loading.value = true;\n    try {\n        const response = await fetch(`${apiUrl}/predict`, {\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json',\n            },\n            body: JSON.stringify({ symptoms: symptoms.value })\n        });\n\n        const result = await response.json();\n        \n        if (result.success) {\n            prediction.value = result.prediction;\n            confidence.value = result.confidence;\n        } else {\n            customAlert('‚ùå Error: ' + result.error);\n        }\n    } catch (error) {\n        customAlert('‚ùå Tidak dapat terhubung ke backend!\\nPastikan backend running di http://localhost:5000');\n    } finally {\n        loading.value = false;\n    }\n};\n\n// Handle reset\nconst handleReset = () => {\n    // Reset state to initial values\n    symptoms.value = { ...initialSymptoms };\n    prediction.value = null;\n    confidence.value = 0;\n};\n\n// Utility function to get dynamic Tailwind classes for color/selection\nconst getColorClasses = (color, isSelected) => {\n    const colors = {\n        orange: isSelected ? 'bg-orange-500 border-orange-600 text-white' : 'border-orange-300 hover:border-orange-400 hover:bg-orange-50',\n        red: isSelected ? 'bg-red-500 border-red-600 text-white' : 'border-red-300 hover:border-red-400 hover:bg-red-50',\n        blue: isSelected ? 'bg-blue-500 border-blue-600 text-white' : 'border-blue-300 hover:border-blue-400 hover:bg-blue-50',\n        purple: isSelected ? 'bg-purple-500 border-purple-600 text-white' : 'border-purple-300 hover:border-purple-400 hover:bg-purple-50',\n        yellow: isSelected ? 'bg-yellow-500 border-yellow-600 text-white' : 'border-yellow-300 hover:border-yellow-400 hover:bg-yellow-50',\n        green: isSelected ? 'bg-green-500 border-green-600 text-white' : 'border-green-300 hover:border-green-400 hover:bg-green-50',\n        pink: isSelected ? 'bg-pink-500 border-pink-600 text-white' : 'border-pink-300 hover:border-pink-400 hover:bg-pink-50',\n        cyan: isSelected ? 'bg-cyan-500 border-cyan-600 text-white' : 'border-cyan-300 hover:border-cyan-400 hover:bg-cyan-50',\n        indigo: isSelected ? 'bg-indigo-500 border-indigo-600 text-white' : 'border-indigo-300 hover:border-indigo-400 hover:bg-indigo-50',\n        gray: isSelected ? 'bg-gray-500 border-gray-600 text-white' : 'border-gray-300 hover:border-gray-400 hover:bg-gray-50'\n    };\n    return colors[color] || colors.blue;\n};\n\nconst getConfidenceColor = (conf) => {\n    if (conf >= 80) return 'from-green-400 to-green-600';\n    if (conf >= 60) return 'from-yellow-400 to-yellow-600';\n    return 'from-orange-400 to-orange-600';\n};\n\nconst getConfidenceBg = (conf) => {\n    if (conf >= 80) return 'bg-green-50 border-green-200';\n    if (conf >= 60) return 'bg-yellow-50 border-yellow-200';\n    return 'bg-orange-50 border-orange-200';\n};\n\nconst getConfidenceTextClass = (conf) => {\n    if (conf >= 80) return 'text-green-600';\n    if (conf >= 60) return 'text-yellow-600';\n    return 'text-orange-600';\n};\n\nconst getConfidenceBadgeClass = (conf) => {\n    if (conf >= 80) return 'bg-green-200 text-green-800';\n    if (conf >= 60) return 'bg-yellow-200 text-yellow-800';\n    return 'bg-orange-200 text-orange-800';\n};\n\n\n// --- Lifecycle Hook ---\nonMounted(() => {\n    checkBackendHealth();\n});\n\n// Watcher untuk mereset prediksi saat gejala berubah (optional, sudah ada di handleSymptomChange)\nwatch(symptoms, () => {\n    // prediction.value = null; // Ini sudah dilakukan di handleSymptomChange\n}, { deep: true });\n\n</script>\n\n<template>\n    <div class=\"min-h-screen bg-gradient-to-br from-slate-900 via-purple-900 to-slate-900 p-4 font-sans\">\n        <div class=\"max-w-7xl mx-auto\">\n            \n            <!-- Backend Status -->\n            <div :class=\"[\n                'mb-4 p-4 rounded-xl shadow-lg backdrop-blur-sm border-2',\n                backendStatus === 'connected' ? 'bg-green-500/20 border-green-400 text-green-100' :\n                backendStatus === 'disconnected' ? 'bg-red-500/20 border-red-400 text-red-100' :\n                'bg-yellow-500/20 border-yellow-400 text-yellow-100'\n            ]\">\n                <div class=\"flex items-center gap-3\">\n                    <component :is=\"backendStatus === 'connected' ? IconCheckCircle2 : backendStatus === 'disconnected' ? IconXCircle : IconLoader2\" \n                               :class=\"['w-5 h-5', backendStatus === 'checking' ? 'animate-spin' : '']\" />\n                    <p class=\"font-semibold\">\n                        Backend Status:\n                        {{ backendStatus === 'connected' ? '‚úÖ Connected to Python Backend' :\n                           backendStatus === 'disconnected' ? '‚ùå Disconnected - Start backend: python backend/app.py' :\n                           'üîÑ Checking connection...'\n                        }}\n                    </p>\n                </div>\n            </div>\n\n            <!-- Header -->\n            <div class=\"bg-gradient-to-r from-purple-600 to-indigo-600 rounded-3xl shadow-2xl p-8 mb-6 border-4 border-purple-400/30\">\n                <div class=\"flex items-center justify-between\">\n                    <div class=\"flex items-center gap-4\">\n                        <div class=\"bg-white/20 backdrop-blur-sm p-4 rounded-2xl border-2 border-white/30\">\n                            <component :is=\"IconPill\" class=\"w-10 h-10 text-white\" />\n                        </div>\n                        <div>\n                            <h1 class=\"text-4xl font-bold text-white mb-1\">üè• Prediksi Penyakit</h1>\n                            <p class=\"text-purple-100 text-lg\">Sistem Decision Tree dengan Python Backend</p>\n                        </div>\n                    </div>\n                    <div class=\"hidden md:flex items-center gap-3 bg-white/10 backdrop-blur-sm px-6 py-3 rounded-2xl border-2 border-white/20\">\n                        <component :is=\"IconHeart\" class=\"w-6 h-6 text-red-300 animate-pulse\" />\n                        <div class=\"text-right\">\n                            <p class=\"text-white font-bold text-2xl\">{{ selectedCount }}</p>\n                            <p class=\"text-purple-200 text-sm\">Gejala Dipilih</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"grid lg:grid-cols-3 gap-6\">\n                <!-- Symptoms Selection -->\n                <div class=\"lg:col-span-2 bg-white/95 backdrop-blur-sm rounded-3xl shadow-2xl p-8 border-4 border-purple-200/50\">\n                    <div class=\"flex items-center justify-between mb-6\">\n                        <h2 class=\"text-2xl font-bold text-gray-800 flex items-center gap-3\">\n                            <div class=\"bg-gradient-to-r from-purple-500 to-pink-500 p-2 rounded-xl\">\n                                <component :is=\"IconCheckCircle2\" class=\"w-6 h-6 text-white\" />\n                            </div>\n                            Pilih Gejala yang Dialami\n                        </h2>\n                        <div class=\"bg-gradient-to-r from-purple-500 to-pink-500 text-white px-5 py-2 rounded-full font-bold text-lg shadow-lg\">\n                            {{ selectedCount }} / {{ symptomsList.length }}\n                        </div>\n                    </div>\n                    \n                    <div class=\"grid sm:grid-cols-2 gap-4\">\n                        <button\n                            v-for=\"{ key, label, icon: Icon, color } in symptomsList\"\n                            :key=\"key\"\n                            @click=\"handleSymptomChange(key)\"\n                            :class=\"[\n                                'p-5 rounded-2xl border-3 transition-all duration-300 text-left transform hover:scale-105 hover:shadow-xl',\n                                getColorClasses(color, symptoms[key]),\n                                symptoms[key] ? 'shadow-lg scale-105' : 'bg-white shadow'\n                            ]\"\n                        >\n                            <div class=\"flex items-center gap-4\">\n                                <div :class=\"[\n                                    'p-3 rounded-xl',\n                                    symptoms[key] ? 'bg-white/30' : `bg-${color}-100`\n                                ]\">\n                    